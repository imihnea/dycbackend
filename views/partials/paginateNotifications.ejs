<div style="margin-left: 1vw;">
    <% if(notifications.page - 1) { %>
        <a class="button page" href="?page=<%= notifications.page - 1 %>">Prev</a>
    <% } %>
    <% if (notifications.page > 4) { %>
        <button class="button page">...</button>
    <% } %>
        <% 
            let k = 0;
            if (notifications.pages > 10) { 
                k = notifications.page + 3;
                while (k > notifications.pages) {
                    k -= 1;
                }
            } else { 
                k = notifications.pages; 
            } 
        %>
        <% for(let i = notifications.page - 3; i <= k; i++) { %>
            <% if (i > 0) { %>
                <a class="button page" href="?page=<%= i %>"
                        <%= i === notifications.page ? 'style=color:#000;' : '' %>
                    ><%= i %></a>
            <% } %>
        <% } %>
        <% if ((notifications.pages > notifications.page) && (k < notifications.pages)) { %>
            <button class="button page">...</button>
        <% } %>
   
    <% if((notifications.page + 1) <= notifications.pages) { %>
        <a class="button page" href="?page=<%= notifications.page + 1 %>">Next</a>
    <% } %>
</div>